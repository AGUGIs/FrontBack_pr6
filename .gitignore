# Создайте .gitignore если его нет
cat > .gitignore << 'EOF'
# Node modules
node_modules/
frontend/node_modules/
backend/node_modules/

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Build outputs
frontend/build/
frontend/dist/
backend/dist/

# Logs
*.log
npm-debug.log*

# OS files
.DS_Store
Thumbs.db

# IDE
.vscode/
.idea/
*.swp
*.swo

# SASS cache
.sass-cache/
EOF